{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-client": "vite",
    "build-client": "tsc -b && vite build",
    "lint-client": "eslint .",
    "preview-client": "vite preview",
    "test:e2e-client": "",
    "test:integration-client": "",
    "test:a11y-client": "",
    "test:unit-client": "",
    "seed": "tsx src/db/seed/seed.ts",
    "start:mongo": "brew services start mongodb-community",
    "start:postgres": "brew services start postgresql",
    "start:redis": "brew services start redis",
    "stop:mongo": "brew services stop mongodb-community",
    "stop:postgres": "brew services stop postgresql",
    "stop:redis": "brew services stop redis",
    "start:services": "concurrently \"npm run start:mongo\" \"npm run start:postgres\" \"npm run start:redis\"",
    "stop:services": "concurrently \"npm run stop:mongo\" \"npm run stop:postgres\" \"npm run stop:redis\"",
    "start-server": "npm run start:services && npm run dev",
    "dev:server": "tsx watch src/index.ts",
    "dev:client-and-server": "concurrently \"cd ../client && npm run dev\" \"tsx watch src/index.ts\"",
    "test:unit": "jest",
    "test:artillery": "artillery run src/tests/performance/artillery.yml",
    "build": "tsc --build",
    "start": "node dist/app.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@commitlint/cli": "^19.7.1",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@tailwindcss/vite": "^4.0.4",
    "@tanstack/react-query": "^5.66.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "react-router": "^7.1.5",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "react-i18next": "^15.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
